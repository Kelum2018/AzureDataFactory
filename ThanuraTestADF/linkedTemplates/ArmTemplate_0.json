				"annotations": [],
				"runtimeState": "Stopped",
				"pipelines": [],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Minute",
						"interval": 1,
						"startTime": "2020-04-30T10:42:00.000Z",
						"endTime": "2020-05-01T10:42:00.000Z",
						"timeZone": "UTC"
					}
				}
			},
			"dependsOn": []
